{
  "type": "record",
  "name": "Publication",
  "namespace": "nl.demeyer.belgiancompanies.publications",
  "doc": "A Belgian Handelsregister publication",
  "fields": [
    {
      "name": "ID",
      "type": "int",
      "doc": "the publication id"
    },
    {
      "name": "DossierNumber",
      "type": "string",
      "doc": "the company dossier number"
    },
    {
      "name": "CompanyName",
      "type": "string",
      "doc": "the company name"
    },
    {
      "name": "Address",
      "type": "string",
      "doc": "the company address"
    },
    {
      "name": "FileLocation",
      "type": "string",
      "doc": "the file location of the publication"
    },
    {
      "name": "DatePublication",
      "type": "string",
      "doc": "the date of publication"
    },
    {
      "name": "Subjects",
      "type": {
        "type": "array",
        "items": "string"
      },
      "doc": "the publication subjects"
    },
    {
      "name": "Raw",
      "type": "string",
      "doc": "the raw html for the publication"
    }
  ]
}
